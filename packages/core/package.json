{
    "name": "@emingy/core",
    "version": "0.0.1",
    "description": "UI-KIT",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "rslib build && rsync -av --exclude '*/story/' src/styles/ dist/styles",
        "check-circular-dependency": "dpdm --no-warning --no-tree --transform --exit-code circular:1 src/**/*.ts",
        "coverage:ts": "type-coverage"
    },
    "author": "Emingy",
    "license": "ISC",
    "exports": {
        "./ui": {
            "types": "./dist/ui/index.d.ts",
            "import": "./dist/ui/index.js",
            "require": "./dist/ui/index.js"
        },
        "./hooks": {
            "types": "./dist/hooks/index.d.ts",
            "import": "./dist/hooks/index.js",
            "require": "./dist/hooks/index.js"
        },
        "./styles/*": "./dist/styles/*"
    },
    "dependencies": {
        "classnames": "2.5.1"
    },
    "typeCoverage": {
        "atLeast": 95,
        "detail": true
    },
    "devDependencies": {
        "@emingy/configs": "workspace:^",
        "@rslib/core": "0.17.0",
        "dpdm": "3.14.0",
        "@rsbuild/plugin-react": "1.4.1",
        "@rsbuild/plugin-sass": "1.4.0",
        "type-coverage": "2.29.7"
    },
    "peerDependencies": {
        "react": "19.2.0",
        "react-dom": "19.2.0"
    }
}
